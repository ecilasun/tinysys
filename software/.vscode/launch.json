	{
		"version": "0.2.0",
		"configurations": [
		  {
			"name": "C/C++(RISCV) - Windows",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/samples/hellodebugger/hellodebugger.elf",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"miDebuggerPath": "C:/riscv32-gcc/bin/riscv32-unknown-elf-gdb.exe",
			"miDebuggerServerAddress": "localhost:1234",
			"setupCommands": [
			  {
				"description": "Enable pretty-printing for gdb",
				"text": "-enable-pretty-printing",
				"ignoreFailures": true
			  }
			]
		  },
		  {
			"name": "C/C++(RISCV) - MacOS",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/samples/hellodebugger/hellodebugger.elf",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"miDebuggerPath": "/Volumes/src/riscv_gcc/bin/riscv32-unknown-elf-gdb",
			"miDebuggerServerAddress": "localhost:1234",
			"setupCommands": [
			  {
				"description": "Enable pretty-printing for gdb",
				"text": "-enable-pretty-printing",
				"ignoreFailures": true
			  }
			]
		  }
		]
	  }